<wf name="case_condition">
	<nodes>
		<node id="StartNode" type="Start" />
		<node id="proc1" type="Process" />
		<node id="proc2" type="Process" />
		<node id="proc3" type="Process" />
		<node id="proc4" type="Process" />
		<node id="EndNode" type="End" />
	</nodes>
	<transitions>
		<transition from="StartNode" to="proc1" />
		<transition from="proc1" to="proc2">
			<rule>[getAge == 25]</rule>
		</transition>
		<transition from="proc1" to="proc3">
			<rule>[getAge &gt; 25]</rule>
		</transition>
		<transition from="proc1" to="proc4">
			<rule>[getAge &lt; 25]</rule>
		</transition>
		<transition from="proc2" to="EndNode" />
		<transition from="proc3" to="EndNode" />
		<transition from="proc4" to="EndNode" />
	</transitions>
</wf>