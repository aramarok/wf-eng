<xflow name="case_and">
    <nodes>
        <node id="StartNode" type="Start"/>
        <node id="proc1" type="Process"/>
        <node id="proc2" type="Process"/>
        <node id="proc3" type="Process"/>
        <node id="AndNode" type="And"/>
        <node id="EndNode" type="End"/>
    </nodes>
    <transitions>
        <transition from="StartNode" to="proc1"/>
        <transition from="StartNode" to="proc2"/>
        <transition from="proc1" to="AndNode"/>
        <transition from="proc2" to="AndNode"/>
        <transition from="AndNode" to="proc3"/>
        <transition from="proc3" to="EndNode"/>
    </transitions>
</xflow>